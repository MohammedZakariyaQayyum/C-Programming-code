using System;
namespace MyApplication
{
 class Program
{
  static void Main(string[] args)
 {
      int myNum = 5;               // integer (whole number)
      long mylongNum = 15000000000L; // longer integer numbers
      double myDoubleNum = 5.99D;  // floating point number with more precision than float(upto 15 decimal digits)
      char myLetter = 'D';         // stores a single character
      bool myBool = true;          // boolean - stores true or false
      string myText = "Hello";     // string
      Console.WriteLine(myNum);
      Console.WriteLine(mylongNum);
      Console.WriteLine(myDoubleNum);
      Console.WriteLine(myLetter);
      Console.WriteLine(myBool);
      Console.WriteLine(myText);
    }
  }
}
